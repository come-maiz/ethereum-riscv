node{
    stage('Test mev-boost') {
        sh '''#!/bin/bash
              rm -rf mev-boost
              git clone https://github.com/flashbots/mev-boost.git
              cd mev-boost
              git checkout stable
              make build
              make test
        '''
    }
    stage('Test trin') {
        sh '''#!/bin/bash
              rm -rf trin
              git clone https://github.com/ethereum/trin.git
              cd trin
	      git submodule update --init
              cargo build --workspace
              cargo test
        '''
    }
}
